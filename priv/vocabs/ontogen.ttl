@prefix og: <https://w3id.org/ontogen#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rtc: <https://w3id.org/rtc#> .


og:Proposition a rdfs:Class
    ; rdfs:subClassOf rtc:Compound
    ; rdfs:label "Ontogen proposition"
    ; rdfs:comment "Represents abstract sets of utterance-independent statements in a canonical form and with content-hashed identifiers."
.


og:SpeechAct a rdfs:Class
    ; rdfs:subClassOf prov:Activity
    ; rdfs:label "SpeechAct", "Sprechakt"@de
    ; rdfs:comment "The utterance of a set of RDF statements, i.e. a RDF graph or dataset."
.

    og:speaker a rdf:Property, owl:FunctionalProperty
        ; rdfs:subPropertyOf prov:wasAssociatedWith, dct:creator
        ; rdfs:domain og:SpeechAct
        ; rdfs:range  prov:Agent
    .

    og:dataSource a rdf:Property
        ; rdfs:domain og:SpeechAct
        ; rdfs:range  dcat:Dataset
    .

    og:insert a rdf:Property, owl:FunctionalProperty
        ; rdfs:domain og:SpeechAct
        ; rdfs:range  og:Proposition
    .

    og:delete a rdf:Property, owl:FunctionalProperty
        ; rdfs:domain og:SpeechAct
        ; rdfs:range  og:Proposition
    .

    og:update a rdf:Property, owl:FunctionalProperty
        ; rdfs:domain og:SpeechAct
        ; rdfs:range  og:Proposition
    .

    og:replace a rdf:Property, owl:FunctionalProperty
        ; rdfs:domain og:SpeechAct
        ; rdfs:range  og:Proposition
    .


og:Commit a rdfs:Class
    ; rdfs:subClassOf prov:Activity
    ; rdfs:label "Ontogen commit"
.

    og:speechAct a rdf:Property, owl:FunctionalProperty
        ; rdfs:subPropertyOf prov:used
        ; rdfs:domain og:Commit
        ; rdfs:range  og:SpeechAct
    .

    og:committedInsert a rdf:Property, owl:FunctionalProperty
        ; rdfs:subPropertyOf prov:used
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Proposition
    .

    og:committedDelete a rdf:Property
        ; rdfs:subPropertyOf prov:used
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Proposition
    .

    og:committedUpdate a rdf:Property, owl:FunctionalProperty
        ; rdfs:subPropertyOf prov:used
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Proposition
    .

    og:committedReplace a rdf:Property, owl:FunctionalProperty
        ; rdfs:subPropertyOf prov:used
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Proposition
    .

    og:committedOverwrite a rdf:Property, owl:FunctionalProperty
        ; rdfs:subPropertyOf prov:used
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Proposition
    .

    og:parentCommit a rdf:Property
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Commit
    .

    og:committer a rdf:Property
        ; rdfs:subPropertyOf prov:wasAssociatedWith
        ; rdfs:domain og:Commit
        ; rdfs:range  og:Agent
    .

    og:commitMessage a rdf:Property
        ; rdfs:domain og:Commit
        ; rdfs:range  rdfs:Literal
    .



og:Agent a rdfs:Class
    ; rdfs:subClassOf prov:Agent, foaf:Agent
    ; rdfs:label "Agent"
.

    og:email a rdf:Property
        ; rdfs:subPropertyOf foaf:mbox
        ; rdfs:domain og:Agent
    .

og:Repository a rdfs:Class
    ; rdfs:label "Ontogen repository"
.

    og:dataset a rdf:Property
        ; rdfs:domain og:Repository
        ; rdfs:range  og:Dataset
    .

    og:provGraph a rdf:Property
        ; rdfs:domain og:Repository
        ; rdfs:range  og:ProvGraph
    .

    og:head a rdf:Property, owl:FunctionalProperty
        ; rdfs:domain og:Repository
        ; rdfs:range  og:Commit
    .

og:Dataset a rdfs:Class
    ; rdfs:subClassOf dcat:Dataset, prov:Entity
    ; rdfs:label "Dataset"
    ; rdfs:comment "A DCAT dataset that version-controlled with Ontogen"
.

    og:repository a rdf:Property
        ; owl:inverseOf og:dataset
        ; rdfs:domain og:Dataset
        ; rdfs:range  og:Repository
        ; skos:scopeNote "This property MAY be used only in addition to its inverse, and it MUST NOT be used to replace it."@en ;
    .

og:ProvGraph a rdfs:Class
    ; rdfs:subClassOf prov:Bundle
    ; rdfs:label "Ontogen provenance graph"
.
